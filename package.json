{
  "name": "cardano-staking-scripts",
  "version": "1.0.0",
  "description": "Cardano staking scripts for Pier Two API",
  "main": "index.js",
  "scripts": {
    "generate-api": "npx swagger-typescript-api generate --path http://localhost:3000/docs-json --output ./generated --name api.ts --unwrap-response-data",
    "add-stake-account": "node -r @swc-node/register add-stake-account.ts",
    "list-stakes": "node -r @swc-node/register list-stakes.ts",
    "register-stake-address": "node -r @swc-node/register register-stake-address.ts",
    "delegate-stake": "node -r @swc-node/register delegate-stake.ts",
    "register-and-delegate": "node -r @swc-node/register register-and-delegate.ts",
    "lint:deps": "knip",
    "check:missing": "knip --include unlisted",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json}\""
  },
  "keywords": ["cardano", "staking", "pier-two"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "dependencies": {
    "@meshsdk/core": "^1.5.0",
    "@meshsdk/react": "^1.5.0",
    "@pier_two/staking-ts": "^1.1.27",
    "dotenv": "^16.5.0",
    "ora": "^8.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/wasm-nodejs": "2.0.0-beta.6",
    "@eslint/js": "^9.27.0",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.11.24",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "knip": "^5.57.0",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^13.2.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
